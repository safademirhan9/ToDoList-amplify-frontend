{
  "function": {
    "AmplifyTodoFunction": {
      "build": true,
      "dependsOn": [
        {
          "attributeEnvMap": {
            "Arn": "TABLE_ARN",
            "Name": "TABLE_NAME"
          },
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "storage",
          "resourceName": "ToDoItemsTable"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "hosting": {
    "S3AndCloudFront": {
      "providerPlugin": "awscloudformation",
      "service": "S3AndCloudFront"
    }
  },
  "parameters": {
    "AMPLIFY_function_AmplifyTodoFunction_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "AmplifyTodoFunction"
        }
      ]
    },
    "AMPLIFY_function_AmplifyTodoFunction_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "AmplifyTodoFunction"
        }
      ]
    }
  },
  "storage": {
    "ToDoItemsTable": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}